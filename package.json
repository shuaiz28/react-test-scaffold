{
  "name": "react-test-scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cypress": "start-server-and-test start http://localhost:1234 cypress:run",
    "test": "NODE_ENV= jest --silent",
    "fix-hooks": "rm -rf .git/hooks && rm -rf node_modules/husky && npm install",
    "prepush": "npm test && npm run jest-coverage-ratchet-auto-commit",
    "jest-coverage-ratchet-auto-commit": "jest-coverage-ratchet && (git add package.json && git commit -m 'AUTO INCREASE TEST COVERAGE') || echo 'continue'",
    "lint": "eslint .",
    "build": "webpack --mode production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.1",
    "history": "^4.10.1",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "cypress": "^4.11.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-coverage-ratchet": "^0.2.3",
    "json-server": "^0.16.1",
    "parcel-bundler": "^1.12.4",
    "path": "^0.12.7",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^1.11.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupFiles/mockSetup.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 10,
        "statements": 10,
        "functions": 10,
        "branches": 10
      }
    }
  }
}
